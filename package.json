{
  "name": "jest-mutator",
  "version": "1.0.0",
  "description": "Mutation tests with Jest",
  "main": "src/index.js",
  "scripts": {
    "start": "ts-node-dev src/index.ts",
    "start:dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
    "test:mutate": "ts-node src/jest.ts",
    "test:watch": "jest --watchAll",
    "lint": "eslint --ext .ts src",
    "prettier:write": "prettier --write \"**/*.(j|t)s\""
  },
  "keywords": [
    "mutator",
    "mutation",
    "jest"
  ],
  "author": "raphael kieling",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "prettier": "3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "pino": "^8.15.6",
    "ts-morph": "^20.0.0",
    "tsimportlib": "^0.0.5"
  }
}
